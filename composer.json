{
    "name": "thopra/guidance",
    "description": "A configurable frontend for thopra/styleguide",
    "type": "thopra-guidance",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/thopra/styleguide.git" },
        { "type": "vcs", "url": "https://github.com/thopra/guidance-installer.git" },
        { "type": "vcs", "url": "https://github.com/thopra/kss-php.git" }
    ],
    "keywords": [
        "kss",
        "styleguide",
        "css documentation"
    ],
    "license": "MIT",
    "require": {
        "thopra/styleguide":"dev-develop",
    	"thopra/guidance-installer":"dev-master"
    },
    "authors": [
        {
            "name": "Thomas Prangenberg",
            "email": "t@thomas-prangenberg.de",
            "homepage": "http://www.thomas-prangenberg.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Thopra\\Guidance\\": "Classes/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
